{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "babel-plugin-transform-async-to-promises",
      {
        "inlineHelpers": true
      }
    ],
    "@babel/plugin-transform-object-assign"
  ]
}
