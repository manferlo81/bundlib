{
  "name": "bundlib",
  "displayName": "Bundlib",
  "version": "0.14.3",
  "description": "A javascript library bundler powered by Rollup.js and optionally Typescript",
  "main": "dist/bundlib.js",
  "module": "dist/bundlib.mjs",
  "bin": "bin/bundlib-cli.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "input": {
      "api": "src/api/index.ts",
      "bin": "src/cli/index.ts"
    },
    "esModule": true,
    "interop": true
  },
  "scripts": {
    "dev": "ts-node -T -P tsconfig-ts-node.json src/cli -dw",
    "build": "ts-node -T -P tsconfig-ts-node.json src/cli -d",
    "test": "jest --color -w 3",
    "clean": "rimraf bin dist types coverage node_modules/.cache/bundlib node_modules/.cache/jest",
    "lint": "eslint src/** __test__/**",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "keywords": [
    "library",
    "bundle",
    "bundler",
    "typescript",
    "rollup"
  ],
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "builtin-modules": "^3.1.0",
    "camelcase": "^5.3.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "commander": "^4.1.1",
    "filesize": "^6.1.0",
    "lodash": "^4.17.15",
    "pretty-ms": "^6.0.1",
    "read-pkg": "^5.2.0",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-export-equals": "^0.3.6",
    "rollup-plugin-strip-shebang": "^1.2.9",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "slash": "^3.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "^13.9.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "mock-fs": "^4.11.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.5",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2"
  },
  "peerDependencies": {
    "rollup": "^2.0.3"
  },
  "files": [
    "bin",
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/bundlib.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/bundlib/issues"
  },
  "homepage": "https://github.com/manferlo81/bundlib#readme",
  "license": "MIT"
}
