{
  "name": "bundlib",
  "version": "0.0.1",
  "description": "A Zero-configuration, automatic javascript library bundler using Typescript and Rollup.js",
  "main": "dist/bundlib.cjs.js",
  "module": "dist/bundlib.es.js",
  "bin": "bin/bundlib-cli.js",
  "typings": "types",
  "scripts": {
    "dev": "ts-node src-bin -dw",
    "build": "ts-node src-bin -d"
  },
  "keywords": [
    "library",
    "bundle",
    "bundler",
    "typescript",
    "rollup"
  ],
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-async-to-promises": "^0.8.8",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "magic-string": "^0.25.2",
    "rollup": "^1.9.0",
    "rollup-plugin-add-shebang": "^0.1.1",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.1.0",
    "rollup-plugin-preserve-shebang": "^0.1.6",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-strip-shebang": "^1.1.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@types/buble": "^0.19.2",
    "@types/node": "^11.13.0",
    "@types/rollup-plugin-buble": "^0.19.1",
    "@types/rollup-plugin-commonjs": "^9.2.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/rollup-plugin-node-resolve": "^4.0.0",
    "ts-node": "^8.0.3"
  },
  "files": [
    "bin",
    "dist",
    "types",
    "src"
  ],
  "author": "Manuel Fernandez <manferlo81@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/bundlib.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/bundlib/issues"
  },
  "homepage": "https://github.com/manferlo81/bundlib#readme",
  "license": "MIT"
}
